{
  "buildCommand": "cd ../.. && pnpm install && pnpm build --filter=@katasumi/web",
  "installCommand": "cd ../.. && pnpm install",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate=300"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/login",
      "destination": "/auth/login",
      "permanent": false
    },
    {
      "source": "/signup",
      "destination": "/auth/signup",
      "permanent": false
    }
  ]
}
